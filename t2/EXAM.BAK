		    printf(".Excellent work!\n");
	    }
	break;

	 case 'b':
	 case 'B':
	    printf("your grade is %c.\n",grade);
	    switch(grade)
	    {
		case 'b':
		case 'B':
		    printf("Well done!\n");
	    }
	break;

	 case 'c':
